
forall Set x {
    x.shape = Circle {
        strokeWidth : 0
    }

    ensure minSize(x.shape)
    ensure maxSize(x.shape)
}

forall Set x; Set y
where IsSubset(x, y) {
    ensure contains(y.shape, x.shape, 5.0)
    x.shape above y.shape
}

forall Set x; Set y
where Not(Intersecting(x, y)) {
    ensure disjoint(x.shape, y.shape)
}
