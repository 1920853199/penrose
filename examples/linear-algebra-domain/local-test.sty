Colors {
    Colors.black = rgba(0.0, 0.0, 0.0, 1.0)
    Colors.gray = rgba(0.6, 0.6, 0.6, 1.0)
    Colors.green = rgba(0.0, 0.8, 0.0, 1.0)
    Colors.blue = rgba(0.0, 0.0, 1.0, 1.0)
    Colors.none = rgba(0.0, 0.0, 0.0, 0.0)
}

const {
      const.shape = Circle {
       	       color : setOpacity(Colors.black, 0.5)
       	       x : 0.0
	       y : 0.0
	       r : 10.0
	       strokeWidth : 0.0
       }
}

Vector v {
       v.x = 0.0
       v.objFn = encourage near(v.shape, const.shape) -- Both go on top
       LOCAL.x = 1.0
       LOCAL.z = v.x + LOCAL.x
       v.shape = Circle {
       	       color : setOpacity(Colors.blue, 0.5)
       	       x : ?
	       y : ?
	       r : ?
	       strokeWidth : 0.0
       }

       v.layering = v.shape above const.shape
}

Vector v; Vector w {
       v.u = 0.0
       LOCAL.u = 0.5
       LOCAL.new = LOCAL.u + v.u
       LOCAL.shape = Circle {
       	       color : setOpacity(Colors.green, 0.5)
       	       x : ?
	       y : ?
	       r : 10.0
	       strokeWidth : 0.0
       }
}