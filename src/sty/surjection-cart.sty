global {
  objective horizontal(X)
  objective upright(Y)
  constraint at(X, 0, 0)
  constraint at(Y, 0, 0)

  objective xInRange(A, B, C, D, 0, 300)
  objective yInRange(A, B, C, D, 0, 300)
}

Set s {
  shape = Arrow {
    label = s.label
  }

  objective above(s, s.label)
}

Point p {
    shape = Dot { 
        label = None 
    }
}
