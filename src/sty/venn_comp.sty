Set x {
  -- constraint contains(x, x.label, computeColorRGBA(1.0, 0.2, 1.0, 0.5))
  constraint contains(x, x.label)
}

Set `A` {
  shape = Circle {
    color = computeColorRGBA(1.0, 0.2, 1.0, 0.5) -- red
  }

  -- TODO: doesn't work; can't find identifier
  -- constraint at(A, 100, 100)
}

Set `B` {
  shape = Circle {
    radius = computeRadiusAsFrac(A, 0.5)
    color = computeColorRGBA(0.1, 1.0, 0.2, 0.4) -- green
  }
}

Subset x y {
    constraint contains(y, x)
    constraint smallerThan(x, y)
    constraint outsideOf(y.label, x)
}
