-- Tree.sty
Set x { 
    shape = Text { }
}

Subset x y {
    objective onTop(y, x)
    objective sameX(x, y)
    shape =  Arrow {
        start = x.shape
        end   = y.shape
        label = None
    }
}

Set x, Set y {
    objective repel(x, y)
}

Subset x S, Subset y S {
    objective sameHeight(x, y)
}
