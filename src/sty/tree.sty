-- Tree.sty
Set x {
    shape = Text {
      string = x.label
    }
}

IsSubset x y {
    objective above(y, x)
    objective equal(x.shape.x, y.shape.y)
    shape =  Arrow {
        start = x.shape
        end   = y.shape
        text  = None
    }
}

Set x, Set y {
    objective repel(x, y)
}

IsSubset x S, IsSubset y S {
    objective sameHeight(x, y)
}
