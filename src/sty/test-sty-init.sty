global { }

Scalar c {
  c.shape = Circ {
    r = 10
  }

  c.labelFn = encourage near(c.shape, c.shape)
}

Scalar `m` {
  `m`.shape.x = 2.5 + 0.2
  `m`.shape.y = `m`.shape.r + `m`.shape.strokeWidth + `m`.shape.x
  
  -- TODO: add more nested computation, test cyclic computation
  
  `m`.inFn = ensure lessThan(`m`.shape.y, 10 + 3)
}